                    <Image url="@/KupidonTeam/fxml/assets/GUI_Parts/Gui_parts/button_cancel.png"/>
                </image>
            </ImageView>
            <ImageView fitHeight="40.0" fitWidth="98.0" layoutX="406.0" layoutY="340.0">
                <image>
                    <Image url="@/KupidonTeam/fxml/assets/GUI_Parts/Gui_parts/name_bar2.png"/>
