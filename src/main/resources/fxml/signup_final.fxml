        <children>
            <ImageView fx:id="stage" fitHeight="457.0" fitWidth="616.0">
                <image>
                    <Image url="@/KupidonTeam/fxml/assets/GUI_Parts/Gui_parts/my_background.png"/>
                </image>
            </ImageView>
            <ImageView fitHeight="302.0" fitWidth="359.0" layoutX="81.0" layoutY="78.0" pickOnBounds="true"
                       preserveRatio="true">
                <image>
                    <Image url="@/KupidonTeam/fxml/assets/signup/woman_and_man.png"/>
                </image>
            </ImageView>
            <ImageView fitHeight="79.0" fitWidth="210.0" layoutX="196.0" layoutY="29.0" pickOnBounds="true"
                       preserveRatio="true">
                <image>
                    <Image url="@/KupidonTeam/fxml/assets/signup/logo_black_bej.png"/>
                </image>
            </ImageView>
            <Label fx:id="usernameLabel" layoutX="361.0" layoutY="96.0" prefHeight="20.0" prefWidth="100.0"
