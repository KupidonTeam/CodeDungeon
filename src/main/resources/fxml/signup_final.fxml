            <ImageView fitHeight="40.0" fitWidth="193.0" layoutX="361.0" layoutY="119.0">
                <image>
                    <Image url="@/KupidonTeam/fxml/assets/GUI_Parts/Gui_parts/name_bar3.png"/>
                </image>
            </ImageView>
            <ImageView fitHeight="40.0" fitWidth="193.0" layoutX="361.0" layoutY="197.0">
                <image>
                    <Image url="@/KupidonTeam/fxml/assets/GUI_Parts/Gui_parts/name_bar3.png"/>
